{
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"editor.formatOnSave": true,
	"editor.quickSuggestions": {
		"strings": true
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"tailwindCSS.includeLanguages": {
		"javascriptreact": "html",
		"plaintext": "html",
		"typescriptreact": "html"
	}
}
