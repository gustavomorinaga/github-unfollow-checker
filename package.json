{
	"name": "github-unfollow-checker",
	"version": "3.1.0",
	"private": true,
	"type": "module",
	"description": "A simple tool to check the users that doesn't follow you back 🧐",
	"author": {
		"name": "Gustavo Morinaga",
		"email": "me@gustavomorinaga.dev",
		"url": "https://gustavomorinaga.dev"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gustavomorinaga/github-unfollow-checker"
	},
	"engines": {
		"pnpm": ">=9",
		"node": ">=22"
	},
	"packageManager": "pnpm@10.7.0",
	"scripts": {
		"dev": "next dev",
		"start": "next start",
		"build": "next build",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "prettier --write .",
		"format:staged": "pretty-quick --staged",
		"test": "jest",
		"test:watch": "jest --watch",
		"type-check": "tsc --noEmit",
		"clean": "del-cli ./**/node_modules/ ./**/.next/ ./**/.vercel/",
		"clean:cache": "del-cli ./**/.next/",
		"clean:modules": "del-cli ./**/node_modules/",
		"clean:vercel": "del-cli ./**/.vercel/",
		"up": "pnpm up --interactive",
		"up-latest": "pnpm up --latest",
		"release": "commit-and-tag-version",
		"release-as-major": "pnpm run release --release-as major",
		"release-as-minor": "pnpm run release --release-as minor",
		"release-as-patch": "pnpm run release --release-as patch",
		"push-release": "git push --follow-tags origin main",
		"pull": "git rebase origin main -i",
		"pre-commit": "pnpm run lint",
		"prepare": "husky",
		"preinstall": "npx only-allow pnpm"
	},
	"dependencies": {
		"@mdx-js/loader": "3.1.0",
		"@mdx-js/react": "3.1.0",
		"@next/mdx": "15.2.4",
		"@radix-ui/react-accordion": "1.2.3",
		"@radix-ui/react-alert-dialog": "1.1.6",
		"@radix-ui/react-avatar": "1.1.3",
		"@radix-ui/react-checkbox": "1.1.4",
		"@radix-ui/react-dropdown-menu": "2.1.6",
		"@radix-ui/react-label": "2.1.2",
		"@radix-ui/react-navigation-menu": "1.2.5",
		"@radix-ui/react-select": "2.1.6",
		"@radix-ui/react-slot": "1.1.2",
		"@rescale/slim": "1.0.1",
		"@tanstack/react-table": "8.21.2",
		"@types/mdx": "2.0.13",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"lucide-react": "0.484.0",
		"motion": "12.6.2",
		"next": "15.2.4",
		"next-auth": "5.0.0-beta.25",
		"next-themes": "0.4.5",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-haiku": "2.2.0",
		"sonner": "2.0.1",
		"tailwind-merge": "3.0.2",
		"tailwindcss-animate": "1.0.7",
		"vaul": "1.1.2"
	},
	"devDependencies": {
		"@commitlint/cli": "19.8.0",
		"@commitlint/config-conventional": "19.8.0",
		"@eslint/eslintrc": "3.3.1",
		"@eslint/js": "9.23.0",
		"@next/eslint-plugin-next": "15.2.4",
		"@tailwindcss/postcss": "4.0.17",
		"@tailwindcss/typography": "0.5.16",
		"@types/node": "22.13.13",
		"@types/react": "19.0.12",
		"@typescript-eslint/eslint-plugin": "8.28.0",
		"@typescript-eslint/parser": "8.28.0",
		"commit-and-tag-version": "12.5.0",
		"del-cli": "6.0.0",
		"eslint": "9.23.0",
		"eslint-config-next": "15.2.4",
		"eslint-config-prettier": "10.1.1",
		"eslint-plugin-prettier": "5.2.5",
		"eslint-plugin-react-hooks": "5.2.0",
		"husky": "9.1.7",
		"lint-staged": "15.5.0",
		"postcss": "8.5.3",
		"prettier": "3.5.3",
		"prettier-plugin-tailwindcss": "0.6.11",
		"pretty-quick": "4.1.1",
		"tailwind-scrollbar": "4.0.1",
		"tailwindcss": "4.0.17",
		"typescript": "5.8.2"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "19.0.12"
		}
	}
}
